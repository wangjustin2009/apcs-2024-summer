#include <stdio.h>
//鍵盤輸入，輸入 2 個數，再輸入 兩數之和，如果正確，顯示“right”，否則顯示“error”
int main()
{
    int a,b,o;
    printf("輸入 2 個數:");
    scanf("%d %d",&a,&b);
    printf("輸入 兩數之和:");
    scanf("%d",&o);    
    if(a+b==o){
        printf("right");
    }
    else{
        printf("error");
    }
    return 0;
}
